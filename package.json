{
  "name": "ptils",
  "version": "0.3.0",
  "description": "utils for package.json",
  "license": "MIT",
  "repository": "akameco/ptils",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "prepare": "yarn build",
    "build": "tsc -p .",
    "fmt": "prettier --write '**/*.{ts,json,md}'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "ci": "yarn lint && yarn test --coverage --ci --runInBand"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "dest/index.js",
  "types": "dest/index.d.ts",
  "files": [
    "dest"
  ],
  "keywords": [
    "package.json",
    "utils",
    "zero"
  ],
  "dependencies": {
    "arrify": "1.0.1",
    "lodash.has": "4.5.2",
    "read-pkg-up": "4.0.0"
  },
  "devDependencies": {
    "@akameco/tsconfig": "^0.1.1",
    "@types/arrify": "^1.0.4",
    "@types/jest": "^24.0.0",
    "@types/lodash.has": "^4.5.4",
    "@types/node": "^10.12.21",
    "@types/read-pkg-up": "^3.0.1",
    "all-contributors-cli": "5.11.0",
    "eslint": "5.13.0",
    "eslint-config-precure": "4.2.0",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-staged": "8.1.3",
    "prettier": "1.16.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
